    public void GenerateExcelSBIFormat(string[] arrCaseID)
    {
        //declaring excel variables
        Excel.Application oExcelApp1;
        Excel.Workbooks oBooks1;
        Excel._Workbook oBook1;
        Excel._Worksheet oSheet1;
        Excel._Worksheet oSheet11;
        Excel.Range oexcelRange1;
        object oMissing1;
        //for getting connection string
        CCommon oCmn;
        try
        {
            String strCentreID = Session["CentreId"].ToString();
            String strClientID = Session["ClientId"].ToString();
            if (strCentreID != "" && strClientID != "")
            {
                oCmn = new CCommon();
                //dataset for main data
                DataSet ds;
                //dataset for attempt details
                DataSet ds1;
                OleDbConnection conn = new OleDbConnection(oCmn.ConnectionString);
                String strRefID = "";
                String strExcel = Server.MapPath("excelSBISocoutput.xls");

                string strMapPath = Server.MapPath("../../ExportToUTI/CC/") + Session["UserName"].ToString() + "/";
                string strDateTime = DateTime.Now.ToString("ddMMyyyyhhmmss");

                if (!Directory.Exists(strMapPath + strDateTime))
                    Directory.CreateDirectory(strMapPath + strDateTime);

                foreach (String strCaseID in arrCaseID)
                {
                    String strTypeID = "";

                    //////////////-------------------------------                  

                    ArrayList arrVerType = new ArrayList();
                    //---------------------------------------------
                    try
                    {
                        //getting main data
                        conn.Open();
                        //String strSQL = "SELECT * FROM CPV_CC_CASE_OUTPUT_VW " +
                        //         "WHERE (CENTRE_ID = '" + strCentreID + "') AND (CLIENT_ID = '" + strClientID + "') AND (CASE_ID = '" + strCaseID + "')";
                        OleDbParameter[] param = new OleDbParameter[3];
                        param[0] = new OleDbParameter("@centre_id", OleDbType.VarChar, 15);
                        param[0].Value = strCentreID;
                        param[1] = new OleDbParameter("@client_id", OleDbType.VarChar, 15);
                        param[1].Value = strClientID;
                        param[2] = new OleDbParameter("@case_id", OleDbType.VarChar, 15);
                        param[2].Value = strCaseID;

                        ds = OleDbHelper.ExecuteDataset(conn, CommandType.StoredProcedure, "SpCPV_CC_CASE_OUTPUT_SBI", param);
                        conn.Close();
                        //excel setting

                        if (ds.Tables[0].Rows.Count > 0)
                            strRefID = ds.Tables[0].Rows[0]["REF_NO"].ToString();
                        String strFileName = strMapPath + strDateTime + "/" + strRefID + ".xls";

                        if (File.Exists(strFileName))
                            File.Delete(strFileName);
                        File.Copy(strExcel, strFileName);

                        oMissing1 = System.Reflection.Missing.Value;
                        oExcelApp1 = new Excel.Application();
                        oExcelApp1.Visible = false;
                        oBooks1 = oExcelApp1.Workbooks;
                        oMissing1 = System.Reflection.Missing.Value;
                        oExcelApp1.Visible = false;
                        oBook1 = oBooks1._Open(strFileName, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1, oMissing1);
                        //oBook = oBooks._Open(strFileName, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing, oMissing);
                        oExcelApp1.Visible = false;
                        oExcelApp1.UserControl = false;

                        //getting data from dataset to insert it in excel cells
                        if (ds != null)
                        {
                            if (ds.Tables[0].Rows.Count > 0)
                            {

                                for (int i = 0; i < ds.Tables[0].Rows.Count; i++)
                                {
                                    //assign ref id
                                    //getting attempt details 
                                    conn.Open();
                                    // SUMMARY
                                    //filling excel according to verification type 
                                    //RV
                                    oSheet1 = (Excel._Worksheet)oBook1.Worksheets["RV"];
                                    arrVerType.Add("RV");
                                    oSheet1.Visible = Excel.XlSheetVisibility.xlSheetVisible;
                                    oSheet1.Unprotect(oMissing1);

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[1, 2]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["APPLICANT_NAME"].ToString();

                                    string MTypeofSociety = "";
                                    MTypeofSociety = ds.Tables[0].Rows[i]["Type_Of_Society"].ToString().Replace("Govt Approved (DDA/ BMP)", "1");
                                    MTypeofSociety = MTypeofSociety.Replace("Other Private Builders", "2");
                                    MTypeofSociety = MTypeofSociety.Replace("Top Private Builders", "3");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[4, 7]);
                                    oexcelRange1.Value2 = MTypeofSociety;

                                    string MOther_Facilities = "";
                                    MOther_Facilities = ds.Tables[0].Rows[i]["Other_Facilities"].ToString().Replace("Maintenance Charges", "1");
                                    MOther_Facilities = MOther_Facilities.Replace("Supermart/parking", "2");
                                    MOther_Facilities = MOther_Facilities.Replace("Gym", "3");
                                    MOther_Facilities = MOther_Facilities.Replace("Swimming Pool", "4");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[5, 7]);
                                    oexcelRange1.Value2 = MOther_Facilities;

                                    string MMarket_Value = "";
                                    MMarket_Value = ds.Tables[0].Rows[i]["Market_Value"].ToString().Replace("35 Lacs", "1");
                                    MMarket_Value = MMarket_Value.Replace("35-50 Lacs", "2");
                                    MMarket_Value = MMarket_Value.Replace("50-75 Lacs", "3");
                                    MMarket_Value = MMarket_Value.Replace("> 75 Lacs", "4");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[6, 7]);
                                    oexcelRange1.Value2 = MMarket_Value;


                                    string MBuildup = "";
                                    MBuildup = ds.Tables[0].Rows[i]["Builtup_Area"].ToString().Replace("400-700 sq ft", "1");
                                    MBuildup = MBuildup.Replace("800-1300 sq ft", "2");
                                    MBuildup = MBuildup.Replace("1300-3000 sq ft", "3");
                                    MBuildup = MBuildup.Replace("> 3000 sq ft", "4");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[7, 7]);
                                    oexcelRange1.Value2 = MBuildup;

                                    string MScoiety = "";
                                    MScoiety = ds.Tables[0].Rows[i]["No_Of_Flats"].ToString().Replace("<20", "1");
                                    MScoiety = MScoiety.Replace("20-50", "2");
                                    MScoiety = MScoiety.Replace("50-100", "3");
                                    MScoiety = MScoiety.Replace("> 100", "4");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[8, 7]);
                                    oexcelRange1.Value2 = MScoiety;

                                    string MBedroom = "";
                                    MBedroom = ds.Tables[0].Rows[i]["Bedroom"].ToString().Replace("YES", "1");
                                    MBedroom = MBedroom.Replace("NO", "2");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[9, 7]);
                                    oexcelRange1.Value2 = MBedroom;

                                    string MNegative = "";
                                    MNegative = ds.Tables[0].Rows[i]["Negative_Area"].ToString().Replace("YES", "1");
                                    MNegative = MNegative.Replace("NO", "1");
                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[10, 7]);
                                    oexcelRange1.Value2 = MNegative;

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[12, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["APPLICANT_NAME"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[13, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["RES_ADD_LINE"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[14, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["Tpc_Name"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[15, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["Tpc_Phone"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[16, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["History"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[18, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["No_Of_Employee"].ToString();

                                    oexcelRange1 = ((Excel.Range)oSheet1.Cells[19, 3]);
                                    oexcelRange1.Value2 = ds.Tables[0].Rows[i]["Remark"].ToString();
                                }
                            }   //if (ds.Tables[0].Rows.Count > 0)
                        }       //(ds != null)
                        ds.Clear();
                        ds.Dispose();
                        ///---------------------
                        oBook1.Save();
                        //saving excel sheet
                        oSheet1 = null;
                        oBook1.Close(false, oMissing1, false);
                        oBook1 = null;
                        oBooks1.Close();
                        oBooks1 = null;
                        oExcelApp1.Quit();
                        oExcelApp1 = null;
                    }
                    catch (Exception exp)
                    {
                        lblMsg.Visible = true;
                        lblMsg.Text = exp.Message;
                        hplDownload.Visible = false;
                        hplDownload.NavigateUrl = "";
                        conn.Close();
                        //throw new Exception(exp.Message);
                    }
                }
                lblMsg.Visible = true;
                lblMsg.Text = "Export Completed successfully.";
                hplDownload.Visible = true;
                hplDownload.NavigateUrl = ConfigurationManager.AppSettings["FtpPath"] + "//CC//" + Session["UserName"].ToString() + "//" + strDateTime + "//";
                conn.Close();
            }
        }
        catch (Exception exp)
        {
            lblMsg.Visible = true;
            lblMsg.Text = exp.Message;
            hplDownload.Visible = false;
            hplDownload.NavigateUrl = "";
            //throw new Exception(exp.Message);            
        }
    }

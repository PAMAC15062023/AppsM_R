    public void GenerateGeSbiFormat(string[] arrCaseID)
    {
        try
        {
            GESBI_ExportGenX GESBI_EX = new GESBI_ExportGenX();
            GESBI_EX.ClientId = Convert.ToInt16(Session["ClientId"]);

            int i = 0;

            string strCaseId = "";


            for (i = 0; i <= gvOutput.Rows.Count - 1; i++)
            {
                System.Web.UI.WebControls.CheckBox chkCaseId = (System.Web.UI.WebControls.CheckBox)gvOutput.Rows[i].Cells[6].FindControl("chkCaseId");
                if (chkCaseId.Checked == true)
                {
                    strCaseId = strCaseId + gvOutput.DataKeys[i].Value.ToString().Trim() + "|";
                }

            }
            strCaseId = strCaseId.Substring(0, strCaseId.Length - 1);
            strCaseId = strCaseId + "^";

            string strFrmDate = "";//Convert.ToDateTime(txtFromDate.Text.Trim()).ToString("yyyyMMdd");
            string strToDate = "";// Convert.ToDateTime(txtToDate.Text.Trim()).ToString("yyyyMMdd");

            //string strFrmDate = Convert.ToDateTime(txtFromDate.Text.Trim()).ToString("yyyyMMdd");
            //string strToDate =  Convert.ToDateTime(txtToDate.Text.Trim()).ToString("yyyyMMdd");

            string FileNamePath = GESBI_EX.GenerateCode(Convert.ToInt32(Session["ClientId"]), strFrmDate, strToDate, strCaseId);
            string[] arrFileName = FileNamePath.Split('\\');

            string strFileName = arrFileName[arrFileName.Length - 1];

            Response.AppendHeader("Content-Disposition", "attachment; filename=" + strFileName.Trim());
            //Response.Headers.Add(FileName,"");
            Response.ContentType = "text/drt";//"application/x-msexcel";
            Response.Flush();

            Response.WriteFile(FileNamePath.Trim(), true);

            HttpContext.Current.Response.End();

            //Response.Flush();
            //Response.Close();
        }

        catch (Exception ex)
        {
            lblMsg.Text = ex.Message;
            HttpContext.Current.Response.End();
        }

    }

